        "zendframework/zftool": "dev-master",
        "doctrine/doctrine-orm-module": "^0.9.2",
        "facebook/php-sdk-v4": "^5.1",
        "zf-commons/zfc-user": "^1.4",
        "zf-commons/zfc-rbac": "~2.4",
